@model EntityProject.Models.ModelBundle

<div class="page-header">
  <h1 class="text-center">Welcome to the Activity Center</h1>
</div>

<div class="container">
  <div class="row">   
    <div class="col-sm-6 vcenter">
      <div id="myCarousel" class="carousel slide" data-ride="carousel">
        <!-- Indicators -->
        <ol class="carousel-indicators">
          <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
          <li data-target="#myCarousel" data-slide-to="1"></li>
          <li data-target="#myCarousel" data-slide-to="2"></li>
        </ol>
        <!-- Wrapper for slides -->
        <div class="carousel-inner">
          <div class="item active">
            <img src="images/hike.jpg" alt="Mount Rainier Full-Day Tour" style="width:100%;">
            <div class="carousel-caption">
              <!-- <h3>Mount Rainier Full-Day Tour</h3> -->
              <p>Evergreen forests & wildflower-filled meadows</p>
            </div>
          </div>
          <div class="item">
            <img src="images/ski.jpg" alt="Skiing" style="width:100%;">
            <div class="carousel-caption">
              <!-- <h3>Chicago</h3> -->
              <p>Natural stunning beauty </p>
            </div>
          </div>
          <div class="item">
            <img src="images/spa.jpg" alt="Spa" style="width:100%;">
            <div class="carousel-caption">
              <!-- <h3>New York</h3> -->
              <p>Mind & body rejuvenation in a relaxing, calm environment</p>
            </div>
          </div>
        </div>
        <!-- Left and right controls -->
        <a class="left carousel-control" href="#myCarousel" data-slide="prev">
          <span class="glyphicon glyphicon-chevron-left"></span>
          <span class="sr-only">Previous</span>
        </a>
        <a class="right carousel-control" href="#myCarousel" data-slide="next">
          <span class="glyphicon glyphicon-chevron-right"></span>
          <span class="sr-only">Next</span>
        </a>
      </div>
      @Html.Partial("Registered.1", Model.model2) 
    </div>
  
    <div class="col-sm-6 vcenter">
      @Html.Partial("Register.1", Model.model1)
      <img src="images/expidia.png" alt="Expedia" style="width:100%;">
    </div>
  </div>
</div>